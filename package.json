{
  "name": "n8n-nodes-zoho-desk",
  "version": "0.1.0",
  "description": "n8n community node for Zoho Desk API integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Pratik Khadloya",
    "email": "tispratik@gmail.com"
  },
  "repository": {
